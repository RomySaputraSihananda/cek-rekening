{
  "swagger": "2.0",
  "info": {
    "title": "Bank-Service",
    "description": "Rest API to check the username with the account number",
    "version": "v1.0.0",
    "license": {
      "name": "Licensed Under MIT",
      "url": "https://spdx.org/licenses/MIT.html"
    }
  },
  "produces": ["application/json"],
  "consumes": ["application/json"],
  "paths": {
    "/api/v1/bank": {
      "get": {
        "x-swagger-router-controller": "bank",
        "operationId": "getBank",
        "tags": ["Bank-Controller"],
        "description": "api for get bankcode and bankname",
        "responses": {
          "200": {
            "description": "200 response",
            "examples": {
              "application/json": { "foo": 1 }
            }
          }
        }
      }
    },
    "/api/v1/ewallet": {
      "get": {
        "x-swagger-router-controller": "ewallet",
        "operationId": "getWallet",
        "tags": ["Bank-Controller"],
        "description": "api for get walletcode and walletname",
        "parameters": [],
        "responses": {
          "200": {
            "description": "200 response",
            "examples": {
              "application/json": "{ foo: 1 }"
            }
          }
        }
      }
    },
    "/api/v1/check": {
      "post": {
        "x-swagger-router-controller": "home",
        "operationId": "index",
        "tags": ["Bank-Controller"],
        "description": "API to check the username",
        "produces": ["application/json"],
        "consumes": ["application/json"],
        "parameters": [
          {
            "in": "body",
            "name": "body",
            "description": "Object codeBank dan nomor Rekening",
            "required": true,
            "schema": {
              "type": "object",
              "example": {
                "bankCode": "002",
                "accountNumber": "616**********33"
              }
            }
          }
        ],
        "responses": {}
      }
    }
  }
}
